.image{
width: 100%;
border-radius: 10rpx;
}
.goods-item{
  text-align: center;
  color: #333;
  padding-bottom: 85rpx;
  
}
.title{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.info .price{
